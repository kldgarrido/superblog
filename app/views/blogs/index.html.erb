<div class="container">
  <div class="row">
    <div class="col-sm-10 col-sm-offset-1">

      
        <% @blogs.each do |blog| %>
        <div>
              <h3><%= link_to blog.title, blog%></h3>
              <h6><%= blog.updated_at %></h6>
              <p><%= blog.body[0..250] %></p>
        </div>
        <% end %>
      
    </div>
  </div>
</div>